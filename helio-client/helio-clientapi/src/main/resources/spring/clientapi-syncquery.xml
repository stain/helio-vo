        http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.1.xsd">
        
    <bean id="syncQueryService" class="eu.heliovo.clientapi.query.syncquery.impl.SyncQueryServiceImpl" scope="prototype" parent="abstractService">
