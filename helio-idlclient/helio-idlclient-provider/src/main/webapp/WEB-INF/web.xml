<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>IDL client service provider</display-name>
  <servlet>
  	<servlet-name>AsyncQueryServiceServlet</servlet-name>
  	<display-name>AsyncQueryServiceServlet</display-name>
  	<description></description>
  	<servlet-class>eu.heliovo.idlclient.provider.servlet.AsyncQueryServiceServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>StaticCatalogRegistryServlet</servlet-name>
  	<display-name>StaticCatalogRegistryServlet</display-name>
  	<description></description>
  	<servlet-class>eu.heliovo.idlclient.provider.servlet.StaticCatalogRegistryServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HelioServiceNameServlet</servlet-name>
  	<display-name>HelioServiceNameServlet</display-name>
  	<description></description>
  	<servlet-class>eu.heliovo.idlclient.provider.servlet.HelioServiceNameServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AsyncQueryServiceServlet</servlet-name>
  	<url-pattern>/AsyncQueryServiceServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>StaticCatalogRegistryServlet</servlet-name>
  	<url-pattern>/StaticCatalogRegistryServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HelioServiceNameServlet</servlet-name>
  	<url-pattern>/HelioServiceNameServlet</url-pattern>
  </servlet-mapping>
</web-app>
