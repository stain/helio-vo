		<param name="time_start"  value="yes" map="lasco_cme_list.time_start"/>
		<param name="description" value="yes" map="lasco_cme_list.description"/>
		<param name="pa_central"  value="yes" map="lasco_cme_list.pa_central"/>
	</mapping>
</map>

