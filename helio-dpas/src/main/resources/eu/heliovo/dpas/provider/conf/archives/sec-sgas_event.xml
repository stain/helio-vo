<?xml version="1.0"  encoding="ISO-8859-1" ?>
<map idArchive="SEC-SGAS" version="2.0">
	<structure>
		<table name="sgas_event">
			<field name="latitude"       type="float"/>
			<field name="longitude"      type="float"/>
			<field name="nar"            type="integer"/>
			<field name="optical_class"  type="string"/>
			<field name="radio_10cm"     type="integer"/>
			<field name="radio_245mhz"   type="integer"/>
			<field name="radio_sweep_ii" type="boolean"/>
			<field name="radio_sweep_iv" type="boolean"/>
			<field name="swf"            type="string"/>
			<field name="time_end"       type="datetime" format="''{0}''"/>
			<field name="time_peak"      type="datetime" format="''{0}''"/>
			<field name="time_start"     type="datetime" format="''{0}''"/>
			<field name="xray_class"     type="string" format="''{0}''"/>
		</table>

	</structure>

	<mapping>
		<param name="event_type"     value="yes" map="IGNORE"/>
		<param name="event-type"     value="yes" map="IGNORE"/>
		<param name="start-date"     value="yes" map="sgas_event.time_start"/>
		<param name="time_start"     value="yes" map="sgas_event.time_start"/>
		<param name="end-date"       value="yes" map="sgas_event.time_end"/>
		<param name="time_end"       value="yes" map="sgas_event.time_end"/>
		<param name="date"           value="no"  begin="sgas_event.time_start" end="sgas_event.time_end"/>
		<param name="latitude"       value="yes" map="sgas_event.latitude"/>
		<param name="longitude"      value="yes" map="sgas_event.longitude"/>
		<param name="nar"            value="yes" map="sgas_event.nar"/>
		<param name="optical_class"  value="yes" map="sgas_event.optical_class"/>
		<param name="radio_10cm"     value="yes" map="sgas_event.radio_10cm"/>
		<param name="radio_245mhz"   value="yes" map="sgas_event.radio_245mhz"/>
		<param name="radio_sweep_ii" value="yes" map="sgas_event.radio_sweep_ii"/>
		<param name="radio_sweep_iv" value="yes" map="sgas_event.radio_sweep_iv"/>
		<param name="swf"            value="yes" map="sgas_event.swf"/>
		<param name="time_peak"      value="yes" map="sgas_event.time_peak"/>
		<param name="xray_class"     value="no" begin="sgas_event.xray_class" end="sgas_event.xray_class"/>
	</mapping>
</map>

