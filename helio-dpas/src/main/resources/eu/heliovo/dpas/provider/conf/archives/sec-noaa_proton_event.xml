<?xml version="1.0"  encoding="ISO-8859-1" ?>
<map idArchive="SEC-NOAA" version="2.0">
	<structure>
		<table name="noaa_proton_event">
			<field name="assoc_cme"      type="string"/>
			<field name="assoc_flare_pk" type="datetime"/>
			<field name="latitude"       type="float"/>
			<field name="longitude"      type="float"/>
			<field name="nar"            type="integer"/>
			<field name="optical_class"  type="string"/>
			<field name="proton_flux"    type="float"/>
			<field name="time_peak"      type="datetime" format="''{0}''"/>
			<field name="time_start"     type="datetime" format="''{0}''"/>
			<field name="xray_class"     type="string" format="''{0}''"/>
		</table>
	</structure>

	<mapping>
		<param name="event-type"     value="yes" map="IGNORE"/>
		<param name="event_type"     value="yes" map="IGNORE"/>
		<param name="date"           value="no"  begin="noaa_proton_event.time_start" end="noaa_proton_event.time_start"/>
		<param name="start-date"     value="yes" map="noaa_proton_event.time_start"/>
		<param name="time_start"     value="yes" map="noaa_proton_event.time_start"/>
		<param name="assoc_cme"      value="yes" map="noaa_proton_event.assoc_cme"/>
		<param name="assoc_flare_pk" value="yes" map="noaa_proton_event.assoc_flare_pk"/>
		<param name="latitude"       value="yes" map="noaa_proton_event.latitude"/>
		<param name="longitude"      value="yes" map="noaa_proton_event.longitude"/>
		<param name="nar"            value="yes" map="noaa_proton_event.nar"/>
		<param name="optical_class"  value="yes" map="noaa_proton_event.optical_class"/>
		<param name="proton_flux"    value="yes" map="noaa_proton_event.proton_flux"/>
		<param name="time_peak"      value="yes" map="noaa_proton_event.time_peak"/>
		<param name="xray_class"     value="no"  begin="noaa_proton_event.xray_class" end="noaa_proton_event.xray_class"/>
	</mapping>
</map>

