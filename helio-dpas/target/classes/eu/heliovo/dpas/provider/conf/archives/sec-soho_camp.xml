<?xml version="1.0"  encoding="ISO-8859-1" ?>
<map idArchive="SEC-SOHO" version="2.0">
	<structure>
		<table name="soho_camp">
			<field name="sohoc_num"   type="integer"/>
			<field name="sohoc_name"  type="string"/>
			<field name="sohoc_type"  type="string"/>
			<field name="sohoc_obj"   type="string"/>
			<field name="sohoc_coord" type="string"/>
			<field name="sohoc_part"  type="string"/>
			<field name="sohoc_comm"  type="string"/>
			<field name="time_end"    type="float" format="''{0}''"/>
			<field name="time_start"  type="datetime" format="''{0}''"/>
		</table>
	</structure>

	<mapping>
		<param name="event-type"  value="yes" map="IGNORE"/>
		<param name="event_type"  value="yes" map="IGNORE"/>
		<param name="start-date"  value="yes" map="time_start"/>
		<param name="time_start"  value="yes" map="time_start"/>
		<param name="end-date"    value="yes" map="time_end"/>
		<param name="time_end"    value="yes" map="time_end"/>
		<param name="date"        value="no"  begin="time_start" end="time_end"/>
		<param name="sohoc_num"   value="yes" map="sohoc_num"/>
		<param name="sohoc_name"  value="yes" map="sohoc_name"/>
		<param name="sohoc_type"  value="yes" map="sohoc_type"/>
		<param name="sohoc_obj"   value="yes" map="sohoc_obj"/>
		<param name="sohoc_coord" value="yes" map="sohoc_coord"/>
		<param name="sohoc_part"  value="yes" map="sohoc_part"/>
		<param name="sohoc_comm"  value="yes" map="sohoc_comm"/>
	</mapping>
</map>

