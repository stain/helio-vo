<?xml version="1.0"  encoding="ISO-8859-1" ?>
<map idArchive="SEC-SRS" version="2.0">
	<structure>
		<table name="srs_list">
			<field name="area"         type="float"/>
			<field name="c_value"      type="string"/>
			<field name="latitude"     type="float"/>
			<field name="long_carr"    type="float"/>
			<field name="long_extent"  type="float"/>
			<field name="longitude"    type="float"/>
			<field name="mag_class"    type="string"/>
			<field name="nar"          type="integer"/>
			<field name="n_spots"      type="integer"/>
			<field name="p_value"      type="string"/>
			<field name="region_type"  type="string"/>
			<field name="time_start"   type="datetime" format="''{0}''"/>
			<field name="zurich_class" type="string"/>
		</table>
	</structure>

	<mapping>
		<param name="event-type"   value="yes" map="IGNORE"/>
		<param name="event_type"   value="yes" map="IGNORE"/>
		<param name="start-date"   value="yes" map="srs_list.time_start"/>
		<param name="time_start"   value="yes" map="srs_list.time_start"/>
		<param name="date"         value="no"  begin="srs_list.time_start" end="srs_list.time_start"/>
		<param name="area"         value="yes" map="srs_list.area"/>
		<param name="c_value"      value="yes" map="srs_list.c_value"/>
		<param name="latitude"     value="yes" map="srs_list.latitude"/>
		<param name="long_carr"    value="yes" map="srs_list.long_carr"/>
		<param name="long_extent"  value="yes" map="srs_list.long_extent"/>
		<param name="longitude"    value="yes" map="srs_list.longitude"/>
		<param name="mag_class"    value="yes" map="srs_list.mag_class"/>
		<param name="nar"          value="yes" map="srs_list.nar"/>
		<param name="n_spots"      value="yes" map="srs_list.n_spots"/>
		<param name="p_value"      value="yes" map="srs_list.p_value"/>
		<param name="region_type"  value="yes" map="srs_list.region_type"/>
		<param name="zurich_class" value="yes" map="srs_list.zurich_class"/>
		
		<addition>
			<select/>
			<from/>
			<where>
				<add value="longitude NOTNULL"/>
			</where>
		</addition>
	</mapping>
</map>

