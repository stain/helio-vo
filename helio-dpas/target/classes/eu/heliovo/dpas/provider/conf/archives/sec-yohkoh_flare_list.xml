<?xml version="1.0"  encoding="ISO-8859-1" ?>
<map idArchive="SEC-YOHKOH" version="2.0">
	<structure>
		<table name="yohkoh_flare_list">
			<field name="hxt_hi"        type="integer"/>
			<field name="hxt_lo"        type="integer"/>
			<field name="hxt_m1"        type="integer"/>
			<field name="hxt_m2"        type="integer"/>
			<field name="latitude"      type="float"/>
			<field name="longitude"     type="float"/>
			<field name="nar"           type="integer"/>
			<field name="optical_class" type="string"/>
			<field name="rem"           type="string"/>
			<field name="time_end"      type="datetime" format="''{0}''"/>
			<field name="time_peak"     type="datetime" format="''{0}''"/>
			<field name="time_start"    type="datetime" format="''{0}''"/>
			<field name="xray_class"    type="string" format="''{0}''"/>
			<field name="yoh_event"     type="integer"/>
		</table>
	</structure>

	<mapping>
		<param name="event-type"    value="yes" map="IGNORE"/>
		<param name="event_type"    value="yes" map="IGNORE"/>
		<param name="start-date"    value="yes" map="yohkoh_flare_list.time_start"/>
		<param name="time_start"    value="yes" map="yohkoh_flare_list.time_start"/>
		<param name="end-date"      value="yes" map="yohkoh_flare_list.time_end"/>
		<param name="time_end"      value="yes" map="yohkoh_flare_list.time_end"/>
		<param name="date"          value="no"  begin="yohkoh_flare_list.time_start" end="yohkoh_flare_list.time_end"/>
		<param name="hxt_hi"        value="yes" map="yohkoh_flare_list.hxt_hi"/>
		<param name="hxt_lo"        value="yes" map="yohkoh_flare_list.hxt_lo"/>
		<param name="hxt_m1"        value="yes" map="yohkoh_flare_list.hxt_m1"/>
		<param name="hxt_m2"        value="yes" map="yohkoh_flare_list.hxt_m2"/>
		<param name="latitude"      value="yes" map="yohkoh_flare_list.latitude"/>
		<param name="longitude"     value="yes" map="yohkoh_flare_list.longitude"/>
		<param name="nar"           value="yes" map="yohkoh_flare_list.nar"/>
		<param name="optical_class" value="yes" map="yohkoh_flare_list.optical_class"/>
		<param name="rem"           value="yes" map="yohkoh_flare_list.rem"/>
		<param name="time_peak"     value="yes" map="yohkoh_flare_list.time_peak"/>
		<param name="xray_class"    value="no" begin="yohkoh_flare_list.xray_class" end="yohkoh_flare_list.xray_class"/>
		<param name="yoh_event"     value="yes" map="yohkoh_flare_list.yoh_event"/>
	</mapping>
</map>

