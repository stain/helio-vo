<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<!-- ============================================================= -->
<!-- Created on Feb 11, 2005                                       -->
<!-- Hibernate mapping for Log class                               -->
<!-- Nathan Ching (nc@mssl.ucl.ac.uk)                              -->
<!-- ============================================================= -->

<hibernate-mapping>

  <class name="org.egso.comms.log.types.Log" table="log">

    <id name="id">
      <generator class="assigned"/>
    </id>

    <property name="name"/>

    <property name="ownerId" column="owner_id"/>

    <property name="timeCreated" column="time_created"/>

  </class>

</hibernate-mapping>