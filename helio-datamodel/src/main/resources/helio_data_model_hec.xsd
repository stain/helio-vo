			<xs:element maxOccurs="1" minOccurs="0" name="frequency_start" type="xs:integer"/>
			<xs:element maxOccurs="1" minOccurs="0" name="frequency_end" type="xs:integer"/>
			<xs:element maxOccurs="1" minOccurs="0" name="circular_polarization_percent" type="xs:integer"/>
			<xs:element maxOccurs="1" minOccurs="0" name="polarization_direction">
