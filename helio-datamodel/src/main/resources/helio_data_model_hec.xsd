			<xs:pattern value="Q[0-5][ S]?"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="ActiveRegion">
		<xs:sequence>
			<xs:element maxOccurs="1" minOccurs="1" name="nar" type="NAR"/>
			<xs:element maxOccurs="1" minOccurs="0" name="area" type="xs:integer"/>
			<xs:element maxOccurs="1" minOccurs="0" name="zurich_class" type="ZurichClass">
				<xs:annotation>
					<xs:documentation>McIntosh classification - first letter Modified Zurich Class</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="1" minOccurs="0" name="p_value">
				<xs:annotation>
					<xs:documentation>McIntosh classification - second letter p-value (assessment of the Largest Spot of the group)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="[AHKRSXahkrsx]"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element maxOccurs="1" minOccurs="0" name="c_value">
				<xs:annotation>
					<xs:documentation>McIntosh classification - third letter p-value (assessment of the Spot Distribution)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="[CIOXciox]"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element maxOccurs="1" minOccurs="0" name="dlong_hg" type="xs:integer">
				<xs:annotation>
					<xs:documentation>Longitudinal extent of the group in heliographic degrees</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="1" minOccurs="0" name="number_sunspots" type="Count"/>
			<xs:element maxOccurs="1" minOccurs="0" name="magnetic_class" type="MagneticClass"/>
			<xs:element maxOccurs="1" minOccurs="1" name="region_type">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="H-ALPHA PLAGES WITHOUT SPOTS"/>
						<xs:enumeration value="REGIONS DUE TO RETURN"/>
						<xs:enumeration value="REGIONS WITH SUNSPOTS"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="ZurichClass">
		<xs:restriction base="xs:string">
			<xs:pattern value="[A-FH]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MagneticClass">
		<xs:restriction base="xs:string">
			<xs:enumeration value="ALPHA"/>
			<xs:enumeration value="BETA"/>
			<xs:enumeration value="BETA-DELTA"/>
			<xs:enumeration value="BETA-GAMMA"/>
			<xs:enumeration value="BETA-GAMMA-DELTA"/>
			<xs:enumeration value="GAMMA"/>
			<xs:enumeration value="GAMMA-DELTA"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="SolarStatistics">
		<xs:sequence>
			<xs:element maxOccurs="1" minOccurs="1" name="time_start" type="Time"/>
			<xs:element maxOccurs="1" minOccurs="1" name="radio_flux_10cm" type="Count"/>
			<xs:element maxOccurs="1" minOccurs="1" name="number_sunspots" type="Count"/>
			<xs:element maxOccurs="1" minOccurs="1" name="sunspot_area" type="xs:integer"/>
