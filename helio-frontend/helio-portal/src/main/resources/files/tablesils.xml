<tab:tables
   xmlns:vr='http://www.ivoa.net/xml/VOResource/v1.0'
   xmlns:vs='http://www.ivoa.net/xml/VODataService/v1.0'
   xmlns:tab='urn:astrogrid:schema:TableMetadata'
   xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
