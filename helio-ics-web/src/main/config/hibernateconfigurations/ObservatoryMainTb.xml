<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>


	<class name="eu.heliovo.ics.common.model.hibernate.ObservatoryMainTb" table="OBSERVATORY">
        <id name="id" column="ID">
            <generator class="native"/>
        </id> 
		<property name="obsId">
		    	<column name="OBS_ID" />
	    </property>
	    <property name="obsName">
		    	<column name="OBS_NAME" />
	    </property>
	    <property name="obsType">
		    	<column name="OBS_TYPE" />
	    </property>
	    <property name="obsFirstPosition">
		    	<column name="OBS_LOC_P1" />
	    </property>
	     <property name="obsSecondPosition">
		    	<column name="OBS_LOC_P2" />
	    </property>
	     <property name="obsStartDate">
		    	<column name="OBS_START_DATE" />
	    </property>
	     <property name="obsEndDate">
		    	<column name="OBS_END_DATE" />
	    </property>
	     <property name="obsOperationType">
		    	<column name="OBS_OPERATION_TYPE" />
	    </property>
	</class>
	
</hibernate-mapping>
