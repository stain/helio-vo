<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="eu.heliovo.ics.common.model.hibernate.InstrumentsMainTb" table="INSTRUMENTS">
       <id name="id" column="ID">
            <generator class="native"/>
        </id> 
        <property name="insId" column="INS_ID"/>        
	    <property name="insObs" column="INS_OBS_ID"/>
	    <property name="insName">
		    	<column name="INS_NAME" />
	    </property>	    
	    <property name="insStartDate">
		    	<column name="INS_START_DATE" />
	    </property>	    
	    <property name="insEndDate">
		    	<column name="INS_END_DATE" />
	    </property>
	</class>
	
</hibernate-mapping>
